{
  "name": "plotly-vue",
  "description": "Plotly js chart wrapper for Vue 3",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --declaration  --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adiglase/plotly-vue.git"
  },
  "keywords": [
    "plotly",
    "vue",
    "chart",
    "data visualization",
    "vue3"
  ],
  "author": "adiglase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adiglase/plotly-vue/issues"
  },
  "homepage": "https://github.com/adiglase/plotly-vue#readme",
  "peerDependencies": {
    "plotly.js-dist-min": "^3.0.0",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.7.2",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.0"
  }
}
